      },
      "require": {
        "composer/installers": "v1.0.12"
      }
    }
  }],
  "require": {
      "composer/installers": "v1.0.12",
      "webdevstudios/custom-metaboxes-and-fields-for-wordpress": "1.2.0"
  },
  "extra": {
    "installer-paths": {
      "public": ["public/includes/single-value-taxonomy-ui"],
      "private": ["admin/includes/cmb4wp"]
    }
  }
}
